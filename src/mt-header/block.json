{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "mt-theme/mt-header",
	"version": "0.1.0",
	"title": "Mt Header",
	"category": "mt-theme",
	"icon": "html",
	"description": "Mt Theme header block.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"logoUrl": {
			"type": "string",
			"default": ""
		},
		"profilePage": {
			"type": "number",
			"default": 0
		},
		"contactPage": {
			"type": "number",
			"default": 0
		}
	},
	"textdomain": "mt-theme",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
